<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.captab">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CapTab">
        <activity android:name=".VerifyPhoneActivity"></activity>
        <activity android:name=".PhoneVerification" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.CapTab.NoActionBar">




        </activity>
        <activity
            android:name=".UserRegistration"
            android:label="@string/app_name"
            android:theme="@style/Theme.CapTab.NoActionBar">



        </activity>
        <activity
            android:name=".OrderDetails"
            android:label="@string/app_name"
            android:theme="@style/Theme.CapTab.NoActionBar" />
        <activity
            android:name=".MyCartView"
            android:label="@string/app_name"
            android:theme="@style/Theme.CapTab.NoActionBar" />
        <activity
            android:name=".TrackOrderStatus"
            android:label="@string/app_name"
            android:theme="@style/Theme.CapTab.NoActionBar" />
    </application>

</manifest>